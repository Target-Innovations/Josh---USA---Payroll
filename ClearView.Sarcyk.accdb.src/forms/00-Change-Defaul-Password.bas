Private Sub cmdCancel_Click()
    
    Forms("00-Login").Visible = True
    DoCmd.Close acForm, Me.Name
    
End Sub

Private Sub cmdSave_Click()

    If Not cSysSettings.oUser.ChangePassword(Me.txtUserName, cSysSettings.oUser.DefaultPwd, Me.txtPassword, Me.txtVerifyPassword) Then
        
        cLogger.LogIt "Failed to change password for :" & Me.txtUserName & "-" & cSysSettings.oUser.Message, EventType.Critical
        MsgBox cSysSettings.oUser.Message, vbCritical
        Me.txtPassword.SetFocus

    Else
        
        cLogger.LogIt "Succeded to change password for :" & Me.txtUserName & "-" & cSysSettings.oUser.Message, EventType.Critical
        MsgBox cSysSettings.oUser.Message, vbInformation
        
        Forms("00-Login").Visible = True
        DoCmd.Close acForm, Me.Name
    
    End If

End Sub

Private Sub Form_Open(Cancel As Integer)

    If cSysSettings.oUser Is Nothing Then Exit Sub
    
    ' If oUser.LogIn(Me.UsernameCMB.Column(1), Me.PasswordTXT) Then
    
    Me.txtEmployee = cSysSettings.oUser.FullName
    Me.txtUserName = cSysSettings.oUser.Username
    
End Sub
